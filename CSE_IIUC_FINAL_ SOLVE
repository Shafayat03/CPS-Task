#include<stdio.h>
int main()
{
    for(int i = 1; i <= 4; i++){
        for(int j = 1; j <= 4; j+=2){
            if(i%2){
                printf("special = retake = fail\n");
            }
        }
    }

  return 0;
}
//output:
/*    
      special = retake = fail
      special = retake = fail
      special = retake = fail
      special = retake = fail

