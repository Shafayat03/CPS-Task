#include<stdio.h>
int main()
{
    int n, result = 0,q,rem;
    scanf("%d",&n);

    q = n;
    while(q != 0){
        rem = q % 10;
        result = result * 10 + rem;
        q /= 10;
    }
    printf("%d\n",result);
    if(result == n)printf("YES\n");
    else{
        printf("NO\n");
    }

  return 0;
}
