#include<stdio.h>
#define SIZE 100010
int main()
{
    int a[SIZE],n,i,j,flag = 0;

    scanf("%d", &n);

    for(i = 0; i < n; i++){
        scanf("%d",&a[i]);
    }
    for(i = 0, j = n-1; i < n/2; i++,j--)
    {
        if(a[i] != a[j]){
            flag = 1;
            break;
        }



    }
    if(flag == 1){
        printf("NO\n");
    }
    else{
        printf("YES\n");
    }





  return 0;
}

