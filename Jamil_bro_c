#include<stdio.h>
#define SIZE 100010
int main()
{
    int a[SIZE],n,i,j,flag = 0;

    scanf("%d", &n);

    for(i = 0; i < n; i++){
        scanf("%d",&a[i]);
    }
    for(i = 0, j = n-1; i < n/2; i++,j--)
    {
        if(a[i] != a[j]){
            flag = 1;
            break;
        }



    }
    if(flag == 1){
        printf("NO\n");
    }
    else{
        printf("YES\n");
    }





  return 0;
}




/// Row wise array input and print
#include<stdio.h>
#define SIZE 100010
int main()
{
    int r, c, a[101][101], i, j;
    scanf("%d %d",&r,&c);

    for(i = 0; i < r; i++)
      {

        for(j = 0;  j < c; j++)
           scanf("%d",&a[i][j]);
      }

   /// output 2D
      for(i = 0; i < r; i++){
        for(j = 0;  j < c; j++)
         printf("%d ",a[i][j]);

         printf("\n");
      }
  return 0;
}

/// Column wise array print

#include<stdio.h>
#define SIZE 100010
int main()
{
    int r, c, a[101][101], i, j;
    scanf("%d %d",&r,&c);

    for(i = 0; i < r; i++)
      {

        for(j = 0;  j < c; j++)
           scanf("%d",&a[i][j]);
      }

   /// output 2D  repesenting the column
      for(i = 0; i < c; i++){
        for(j = 0;  j < r; j++)
         printf("%d ",a[j][c]);

         printf("\n");
      }

/// we use this also if we want to store i in c and j in column
/**   

  for(j = 0; i < c; j++){
        for(i = 0;  i < c; i++)
         printf("%d ",a[i][j]);

         printf("\n");
      }




*/





  return 0;
}


