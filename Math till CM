   for(int i = 1; i * i <= n;i++){

       if(n % i == 0){
        cnt++;  /// i is a factor

        if( i != n / i)
            cnt++;   /// (n / i) is also a factor


       }



   }

   cout << cnt << endl;

   */



   /// finding the divisors  --- > itarates on numbers

///complexityy : O(nlogN)

  /*
