
///  Harmony Series -- >   PreComputation technique
/**
     1 - 10  er modde songkha gula r divisor      koi tah ace konta r jonno eta count kora


*/
/// In the name of Allah
#include<bits/stdc++.h>
#define endl '\n'
using namespace std;

const int mx = 1e7 + 123;

int cnt[mx];


int main()
{

        int lim = 10;

        for(int i = 1; i <= 10; i++)
        {
            for(int  j = i ; j <= 10;  j += i)
            {
                cnt[j]++;
            }
        }


    for(int i = 1; i <= 10; i++){
        cout << i << ": " << cnt[i] << endl;
    }


}
