#include<stdio.h>
int main()
{
    int tc;
    scanf("%d",&tc);
    while(tc--)
    {
        int n;
        scanf("%d",&n);
        if(n == 0)printf("0");

        while(n > 0)
        {
            int r = n % 10;
             printf("%d ",r);
             n = n / 10;

        }
         printf("\n");


    }








  return 0;
}
