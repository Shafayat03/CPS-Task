/// Minimum value print in array


#include<stdio.h>
int main()
{
    int num[100123],n;
    scanf("%d",&n);

    for(int i = 0; i < n; i++){
        scanf("%d",&num[i]);
    }

    int mn = num[0];

    for(int i = 1; i < n; i++){
        if( mn > num[i])
            mn = num[i];

    }
    printf(" min : %d\n", mn);



  return 0;
}
