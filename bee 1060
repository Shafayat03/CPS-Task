#include<stdio.h>
int main()
{
    double a;
    int i, j = 0;
    for(i = 1; i <= 6; i++){
        scanf("%lf",&a);
        if(a > 0){
            j++;
        }
    }
    printf("%d valores positivos\n",j);

  return 0;
}

//another approach will b e count if we declare 3 double variable and from them a  initial value is zero and if the value of that  variable is positive  we use variable++. 
