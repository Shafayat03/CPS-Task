#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t = 1; cin >>t;
    while(t--)
    {
        string s; s = "codeforces";
        char c; cin >>c;
        int flag = 0;
        for(int i = 0; i  <s.size(); i++){
            if(c==s[i])flag = 1;

        }
        if(flag) cout << "YES" << endl;
        else cout << "NO" << endl;



    }

}
