
///1. Write a program to sort n numbers using Insertion Sort algorithm.

#include<bits/stdc++.h>
using namespace std;
using ll = long long;
//#define mx 1e5 + 123
void insertion_sort_algo(int a[], int n){

   int i , key_val, j;

   for(int i = 1; i < n; i++){
       key_val = a[i];

       j = i - 1;

       // akn [0...i -1] theke check korbo key_val_ pacci kina

       while(j >= 0 and a[j] > key_val){

          a[j+1] = a[j];
          j--; // j er value komabo

       }
       a[j+1] = key_val; // ai position e key_val k store korbo
   }




}


int main(){

      int n;
      cin >> n;

      int a[100000+123];

      for(int i = 0; i < n; i++){
        cin >> a[i];
      }


      insertion_sort_algo(a, n);

        for(int i = 0; i < n; i++){
        cout <<  a[i] << ' ';
      }
      cout << '\n';




return 0;
}






///2. Write a program to sort n numbers using Selection Sort algorithm.

#include<bits/stdc++.h>
using namespace std;
using ll = long long;
//#define mx 1e5 + 123

void selection_sort_algo(int a[], int n){



   for(int i = 0; i < n -1; i++){

       int mn = i;

       for(int j = i + 1; j < n ;j++){
            if(a[j] < a[mn]){
                  mn = j;
            }
       }

     if(mn != i){
         int t = a[mn];
         a[mn] = a[i];
         a[i] = t;
     }


   }
}
int main(){

      int n;
      cin >> n;

      int a[100000+123];

      for(int i = 0; i < n; i++){
        cin >> a[i];
      }


      selection_sort_algo(a, n);

        for(int i = 0; i < n; i++){
        cout <<  a[i] << ' ';
      }
      cout << '\n';




return 0;
}





